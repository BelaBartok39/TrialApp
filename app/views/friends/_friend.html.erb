<div id="<%= dom_id friend %>">


<div class="jumbotron">
  <h1 class="display-4"><%= friend.first_name %> <%= friend.last_name %></h1>
  <h3><%= friend.phone %></h3>
  <hr class="my-4">
  <p class="lead"><%= friend.email %></p>
  <%= link_to "Edit Contact", edit_friend_path(@friend), class:"btn btn-secondary" %>
  <%= link_to "Back to Contacts", friends_path, class:"btn btn-secondary" %>
</div>



</div>
